    "limit = 4000000\n",
    "sum = 0\n",
    "first = 1\n",
    "second = 2\n",
    "\n",
    "while second < limit:\n",
    "    if second % 2 == 0:\n",
    "        sum += second\n",
    "\n",
    "    next = first + second\n",
    "    first,second = second,next\n",
    "\n",
    "print (sum)"
